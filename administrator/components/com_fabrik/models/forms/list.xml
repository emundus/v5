				default="1"
				description="COM_FABRIK_FIELD_VIEW_RECORDS_DESC"
				label="COM_FABRIK_FIELD_VIEW_RECORDS_LABEL" />
				
			<field name="allow_edit_details"
				type="accesslevel"
