				default="0"
				description="COM_FABRIK_FIELD_CSV_INCLUDE_CALCULATIONS_DESC"
				label="COM_FABRIK_FIELD_CSV_INCLUDE_CALCULATIONS_LABEL" >
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
				
