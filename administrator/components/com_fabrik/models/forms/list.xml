				label="COM_FABRIK_FIELD_RSS_LIMIT_LABEL" />
				
			<field name="rsslimitmax"
				type="text"
				class="input-mini"
				default="2500"
				description="COM_FABRIK_FIELD_RSS_LIMIT_MAX_DESC"
				label="COM_FABRIK_FIELD_RSS_LIMIT_MAX_LABEL"
