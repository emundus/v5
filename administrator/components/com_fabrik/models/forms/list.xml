			<field name="allow_delete2"
				type="listfields"
				description="COM_FABRIK_FIELD_OR_USE_FIELD_DELETE_LABEL"
				label="COM_FABRIK_FIELD_OR_USE_FIELD_LABEL"
				valueformat="tableelement"/>
			
			<field name="allow_drop"
