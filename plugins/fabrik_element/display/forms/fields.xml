<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<form>
	<fields>
		<fieldset name="plg-element-display">
		
			<field type="helplink"
				url="PLG_ELEMENT_DISPLAY_HELP_SERVER" />
				
			<field default="0" label="PLG_ELEMENT_DISPLAY_EVAL_LABEL" name="eval" type="radio">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
				
			<field cols="45" label="PLG_ELEMENT_DISPLAY_DEFAULT_LABEL" name="default" rows="6" type="textarea"/>
			
		</fieldset>
	</fields>
	
	<fields name="params">
		<fieldset name="extra">
			<field default="1" label="PLG_ELEMENT_DISPLAY_SHOW_LABEL_LABEL" name="display_showlabel" type="radio">
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
		</fieldset>
	</fields>
</form>
