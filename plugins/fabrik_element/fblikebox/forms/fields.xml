<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<form>
	<fields addfieldpath="/administrator/components/com_fabrik/models/fields" name="params">
		<fieldset label="field" name="plg-element-fblikebox">
		
			<field type="helplink"
				url="PLG_ELEMENT_FBLIKEBOX_HELP_SERVER" />
				
			<field description="PLG_ELEMENT_FBLIKEBOX_PAGE_ID_DESC" label="PLG_ELEMENT_FBLIKEBOX_PAGE_ID_LABEL" name="fblikebox_pageid" type="text"/>
				
			<field default="300" label="PLG_ELEMENT_FBLIKEBOX_WIDTH_LABEL" name="fblikebox_width" size="3" type="text"/>
				
			<field default="300" label="PLG_ELEMENT_FBLIKEBOX_HEIGHT_LABEL" name="fblikebox_height" size="3" type="text"/>
				
			<field default="true" label="PLG_ELEMENT_FBLIKEBOX_HEADER_LABEL" name="fblikebox header" type="radio">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
			
			<field default="true" label="PLG_ELEMENT_FBLIKEBOX_STREAM_LABEL" name="fblikebox_stream" type="radio">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
			
			<field default="10" label="PLG_ELEMENT_FBLIKEBOX_CONNECTIONS_LABEL" name="fblikebox_connections" type="text"/>
					
		</fieldset>

	</fields>
</form>
